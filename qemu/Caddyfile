{
	order sablier first
}

# Example Built-in Web Service
:8080 {
	handle /tailnet {
		respond "Tailnet System Core" 200
	}
	handle /status {
		respond "READY" 200
	}
	handle {
		respond "Tailnet Built-in Service Example" 200
	}
}

# Sablier Admin / Smoke Test for Legacy Compatibility
:10000 {
	handle /health {
		respond "OK" 200
	}
	handle {
		reverse_proxy localhost:2019
	}
}
